&mt {
    quick-tap-ms = <100>;
    global-quick-tap;
    flavor = "tap-preferred";
    tapping-term-ms = <170>;
};

&num_word {
    continue-list = <
    BSPC DEL
    DOT COMMA
    PLUS MINUS STAR FSLH EQUAL
    // J K AT_SIGN V G LS(G)
    >;
};

&mmv_input_listener {
    fast {
        layers = <FMMV>;
        input-processors = <&zip_xy_scaler 2 1>;
    };
};

